@model List<string>
@{
    ViewBag.Title = "_Grid";
}

<h2>Movies Grid</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Title</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @item
                </td>
                <td>
                    Details
                </td>

            </tr>
        }
    </tbody>
</table>